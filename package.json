{
    "name": "@daniel3711997/quick",
    "description": "Integrates React JS with WordPress",
    "private": true,
    "type": "module",
    "version": "1.0.0",
    "engines": {
        "npm": ">=9.0.0",
        "node": ">=18.0.0"
    },
    "author": {
        "name": "Daniel3711997",
        "url": "https://github.com/Daniel3711997",
        "email": "onofreicostineldanut12@gmail.com"
    },
    "scripts": {
        "dev": "check-engine && concurrently -r -k \"typed-scss-modules \\\"**/*.module.scss\\\" -L silent -n camel -e named -w -o .generated\" \"vite\"",
        "build": "tsc && vite build",
        "postbuild": "composer dump-autoload --optimize && node scripts/_postbuild.cjs",
        "prebuild": "check-engine && vitest run __tests__/js && php ./vendor/bin/phpunit __tests__/php --testdox && ./vendor/bin/phpcs index.php app __tests__/php --standard=.php-style.xml && cspell --no-progress \"{app,src,__tests__}/**/*.{php,js,jsx,ts,tsx,css,scss,json}\""
    },
    "overrides": {
        "check-engine": {
            "semver": "^7.0.0"
        },
        "css-modules-loader-core": {
            "postcss": "^7.0.0"
        }
    },
    "dependencies": {
        "@tanstack/react-query": "^4.29.19",
        "@tanstack/react-query-devtools": "^4.32.0",
        "clsx": "^2.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.10"
    },
    "devDependencies": {
        "@tanstack/eslint-plugin-query": "^4.29.25",
        "@testing-library/react": "^14.0.0",
        "@types/node": "^20.4.5",
        "@types/react": "^18.2.16",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@vitejs/plugin-react": "^4.0.3",
        "chalk": "^4.1.2",
        "check-engine": "^1.11.0",
        "concurrently": "^8.2.0",
        "cspell": "^6.31.2",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.3",
        "eslint-plugin-sort-exports": "^0.8.0",
        "prettier": "^3.0.0",
        "sass": "^1.64.1",
        "stylelint": "^15.10.2",
        "stylelint-config-clean-order": "^5.0.1",
        "stylelint-config-sass-guidelines": "^10.0.0",
        "typed-scss-modules": "^7.1.2",
        "typescript": "^5.1.6",
        "vite": "^4.4.7",
        "vite-plugin-eslint": "^1.8.1",
        "vite-plugin-stylelint": "^4.3.0",
        "vite-tsconfig-paths": "^4.2.0",
        "vitest": "^0.33.0"
    },
    "quickApplicationNodeEnvironment": "production",
    "quickApplicationPublicPath": "/wp-content/plugins/quick/build"
}